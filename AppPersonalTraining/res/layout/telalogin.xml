<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout
	android:id="@+id/widget0"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:background="#fff5f5f5"
	xmlns:android="http://schemas.android.com/apk/res/android">
<EditText
	android:id="@+id/txtEmailLogin"
	android:layout_width="157dp"
	android:layout_height="50dp"
	android:hint="E-mail"
	android:textSize="18sp"
	android:layout_x="77dp"
	android:layout_y="79dp" />
<EditText
	android:id="@+id/txtSenhaLogin"
	android:layout_width="157dp"
	android:layout_height="51dp"
	android:hint="Senha"
	android:textSize="18sp"
	android:password="true"
	android:layout_x="76dp"
	android:layout_y="148dp" />
<Button
	android:id="@+id/btLogin"
	android:layout_width="160dp"
	android:layout_height="48dp"
	android:text="Logar"
        android:onClick="onClickbtLogin"
	android:layout_x="71dp"
	android:layout_y="218dp" />
<Button
	android:id="@+id/btCadastro"
	android:layout_width="160dp"
	android:layout_height="48dp"
	android:text="Cadastre-se"
        android:onClick="onClickbtCadastro"
	android:layout_x="71dp"
	android:layout_y="292dp" />
</AbsoluteLayout>
